{
  "name": "devook-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "debounce": "^1.2.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.6.0",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.33.1",
    "react-query": "^3.34.7",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
    "recoil": "^0.5.2",
    "styled-components": "^5.3.3",
    "styled-reset": "^4.3.4",
    "typescript": "^4.4.2",
    "wait-on": "^6.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "pretty": "prettier --write \"src/**/*.(ts|tsx)\"",
    "tsc": "tsc",
    "test": "yarn lint && yarn pretty && yarn tsc && yarn jest",
    "cy:open": "yarn start & wait-on https://localhost:3000 && cypress open",
    "cy:run": "yarn start & wait-on https://localhost:3000 && cypress run --browser chrome",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.10",
    "@storybook/addon-essentials": "^6.4.10",
    "@storybook/addon-links": "^6.4.10",
    "@storybook/builder-webpack5": "^6.4.10",
    "@storybook/manager-webpack5": "^6.4.10",
    "@storybook/node-logger": "^6.4.10",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.10",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.178",
    "@types/react-datepicker": "^4.3.4",
    "@types/react-outside-click-handler": "^1.3.1",
    "@types/styled-components": "^5.1.19",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "cypress": "^9.4.1",
    "cypress-react-selector": "^2.3.15",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "webpack": "^5.65.0"
  }
}
